{
  "name": "atasnalar-bootstrap",
  "version": "1.0.1",
  "description": "This Sass is based on Bootstrap ^5.2.3. we don't recommend editing the Sass files directly in this folder. This file has been re-implemented by Atas Nalar with some changes and will be overwritten by us when \"New Updates\" from Bootstrap are released.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:sass": "sass --no-source-map --load-path=node_modules scss:css",
    "watch:sass": "sass --watch --no-source-map --load-path=node_modules scss:css",
    "build:prefix": "autoprefixer css/*.css",
    "watch:prefix": "autoprefixer --watch css/*.css",
    "build:css": "minify css/*.css --output css/*.min.css",
    "watch:css": "minify --watch css/*.css --output css/*.min.css",
    "build:js": "minify js/*.js --output js/*.min.js",
    "watch:js": "minify --watch js/*.js --output js/*.min.js",
    "build": "npm-run-all build:*",
    "watch": "npm-run-all watch:*",
    "start": "npm-run-all build watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awpdesigns/atasnalar-bootstrap.git"
  },
  "author": "Atas Nalar <atasnalar@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "minify": "^9.1.0",
    "sass": "^1.49.8",
    "npm-run-all": "^4.1.5"
  },
  "bugs": {
    "url": "https://github.com/awpdesigns/atasnalar-bootstrap/issues"
  },
  "homepage": "https://github.com/awpdesigns/atasnalar-bootstrap#readme"
}
